/* package whatever; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
		
		
		 int a = 100; // Начальное значение диапазона - "от"
      int b = 999; // Конечное значение диапазона - "до"
      
      int random_number1 = a + (int) (Math.random() * b); // Генерация случайного трехзначного числа
      System.out.println("Случайное число: " + random_number1);
      
    
      {
      System.out.println("Максимальное число: " + Math.max(a, b));  //Выводим максимальное из диапазона 
	}
     
      
      
      
      
	}
	
	
}
